{
  "port": 8000,
  "admin": false,
  "log_level": "info",
  "allowed_origins": ["*"],
  "proxy_connect_endpoint": "${PROXY_CONNECT_ENDPOINT}",
  "proxy_subscribe_endpoint": "${PROXY_SUBSCRIBE_ENDPOINT}",
  "proxy_publish_endpoint": "${PROXY_PUBLISH_ENDPOINT}",
  "proxy_connect_timeout": "5s",
  "proxy_subscribe_timeout": "5s",
  "proxy_publish_timeout": "5s",
  "namespaces": [
    {
      "name": "chat",
      "presence": true,
      "join_leave": true,
      "history_size": 100,
      "history_ttl": "60s",
      "force_recovery": false,
      "proxy_subscribe": true,
      "proxy_publish": true
    }
  ],
  "health": true
}
